<% include ../include/header %>
<div id="wrapper">
    <% include ../include/nav %>
    <div id="page-wrapper" class="gray-bg dashbard-1">
        <% include ../include/navbar %>
        <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>财务报告</h5>
                        </div>
                        <div class="ibox-content">
                            <form class="form-horizontal m-t" action="/finance/add" method="post" enctype="multipart/form-data">
                                <input type="hidden" name="clientId">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">请上传.csv格式的文件：</label>
                                    <div class="col-sm-8">
                                        <input id="csv" name="csv" class="form-control" type="file" accept=".csv">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-8 col-sm-offset-3">
                                        <button class="btn btn-primary" type="submit">提交</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <% include ../include/copy %>
    </div>
</div>

<% include ../include/footer %>
